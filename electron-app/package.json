{
  "name": "electron-quick-start-typescript",
  "appname": "FIT CCC Input Automation",
  "appVersion": "0.1.0",
  "homepage": "./",
  "version": "0.1.0",
  "main": "./dist/main.js",
  "description": "Automation for CCC by FIT",
  "scripts": {
    "prebuild:react": "rimraf -rf build",
    "build:react": "npm run --prefix ../react-app build",
    "postbuild:react": "copyfiles -u 3 ../react-app/build/**/* build",
    "prebuild:electron": "rimraf -rf dist",
    "build:electron": "tsc",
    "package": "electron-packager . \"FIT CCC Input Automation\" --out=\"release-app\" --icon=./icon.ico --overwrite",
    "build": "npm run build:react && npm run build:electron && npm run package",
    "dev:watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "start": "cross-env NODE_ENV=development electron ./dist/main.js",
    "electron": "cross-env NODE_ENV=development electron ./dist/main.js",
    "dev:react": "npm run --prefix ../react-app start",
    "dev": "concurrently \"npm:dev:*\""
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.2",
    "electron": "^13.1.6",
    "electron-packager": "^15.3.0",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.30.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@nut-tree/nut-js": "^1.7.0",
    "axios": "^0.21.1",
    "concurrently": "^6.2.0",
    "electron-store": "^8.0.0",
    "rimraf": "^3.0.2"
  },
  "files": [
    "./build/**/*",
    "./dist/**/*",
    "./node_modules/**/*"
  ],
  "build": {
    "appId": "ccc.id",
    "mac": {
      "category": "ccc.category"
    },
    "protocols": {
      "name": "ccc-protocol",
      "schemes": [
        "ccc"
      ]
    }
  }
}
